{
  "name": "gwcloud-auth",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --mode development --hot ",
    "start_win": "set \"NODE_ENV=development\" && webpack-dev-server --mode development --hot ",
    "build": "NODE_ENV=production webpack --mode production",
    "build_win": "set \"NODE_ENV=production\" && webpack --mode production",
    "relay": "relay-compiler --src ./src --schema ../../gwcloud-auth/src/react/data/schema.json --extensions=js --extensions=jsx",
    "relay-ci": "relay-compiler --src ./src --schema .data/schema.json --extensions=js --extensions=jsx",
    "test": "jest",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@jest/globals": "^26.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "is-promise": "latest",
    "lodash": "latest",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "latest",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "warning": "latest",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.4.4",
    "@restart/context": "latest",
    "@testing-library/react": "^11.0.4",
    "babel-plugin-relay": "^9.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.5.2",
    "eslint": "^7.11.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-react": "^7.21.4",
    "farce": "^0.2.8",
    "found": "^0.4.9",
    "found-relay": "^0.6.0",
    "graphql": "^14.6.0",
    "jest": "^26.1.0",
    "jest-junit": "^11.0.1",
    "jest-watch-typeahead": "^0.6.1",
    "jquery": "^3.5.1",
    "linq": "^3.2.1",
    "prop-types": "latest",
    "react-bootstrap": "^1.3.0",
    "react-icons": "^3.11.0",
    "react-relay": "^9.0.0",
    "react-relay-network-modern": "^4.4.1",
    "react-static-container": "latest",
    "relay-compiler": "^9.0.0",
    "relay-runtime": "^9.0.0",
    "relay-test-utils": "^10.0.0",
    "stylelint": "^13.7.2",
    "stylelint-scss": "^3.18.0",
    "url-loader": "^3.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "collectCoverage": true,
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  }
}

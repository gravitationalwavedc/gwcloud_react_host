{
  "name": "gwcloud-react-host",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start_gwcloud": "npm run relay && npx cross-env FORCE_DOMAIN=gwcloud NODE_ENV=development webpack serve --mode development --hot",
    "start_gwlab": "npm run relay &&  npx cross-env FORCE_DOMAIN=gwlab NODE_ENV=development webpack serve --mode development --hot",
    "start_gwlandscape": "npm run relay &&  npx cross-env FORCE_DOMAIN=gwlandscape NODE_ENV=development webpack serve --mode development --hot",
    "build": "npm run relay &&  npx cross-env NODE_ENV=production webpack --mode production",
    "relay": "relay-compiler --src ./src --schema ../../gwcloud_auth/src/react/data/schema.json --extensions=js --extensions=jsx",
    "relay-ci": "relay-compiler --src ./src --schema .data/schema.json --extensions=js --extensions=jsx",
    "test": "npm run relay && jest",
    "test-ci": "jest --ci --reporters=default --reporters=jest-junit"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.13.10",
    "@jest/globals": "^26.6.2",
    "@popperjs/core": "^2.9.1",
    "@restart/context": "2.1.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.7",
    "babel-loader": "^8.2.2",
    "babel-plugin-relay": "^11.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bootstrap": "^4.6.0",
    "core-js": "^3.9.1",
    "css-loader": "^5.1.2",
    "eslint": "^7.22.0",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-react": "^7.22.0",
    "farce": "^0.4.5",
    "found": "^0.6.0",
    "found-relay": "^0.9.0",
    "graphql": "^15.5.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "is-promise": "4.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "jquery": "^3.6.0",
    "linq": "^3.2.3",
    "lodash": "4.17.21",
    "node-sass": "^5.0.0",
    "prop-types": "15.7.2",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "react-redux": "7.2.2",
    "react-relay": "^11.0.0",
    "react-relay-network-modern": "^6.0.0",
    "react-static-container": "1.0.2",
    "react-test-renderer": "^17.0.1",
    "relay-compiler": "^11.0.0",
    "relay-runtime": "^11.0.0",
    "relay-test-utils": "^11.0.0",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-scss": "^3.19.0",
    "url-loader": "^4.1.1",
    "warning": "4.0.3",
    "webpack": "^5.25.1",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "collectCoverage": true,
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest",
      "^.+\\.css$": "<rootDir>/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "devDependencies": {
    "stylelint-config-recommended": "^9.0.0"
  }
}
